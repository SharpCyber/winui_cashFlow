                <ResourceDictionary Source="ms-appx:///Styles/ButtonStyle.xaml"/>
                <ResourceDictionary Source="ms-appx:///Styles/FontIconStyle.xaml"/>
                <ResourceDictionary Source="ms-appx:///Styles/TextBlockStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
